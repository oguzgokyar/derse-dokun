{
    "name": "Derse Dokun",
    "version": "0.1",
    "description": "Google Meet'i etkileşimli bir sınıfa çevirir.",
    "permissions": [
        "activeTab",
        "identity"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/*"
            ],
            "js": [
                "ui.js",
                "firebase-app.js",
                "firebase-firestore.js"
            ]
        }
    ],
    "manifest_version": 2
}